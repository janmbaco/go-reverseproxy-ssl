        "allow_all_origins": true,
        "allowed_origins": [],
        "use_web_sockets": false,
        "allowed_headers": []
      }
    }
  ],
  "ssh_virtual_hosts": [],
  "default_host": "localhost",
  "reverse_proxy_port": ":443",
  "config_ui_port": ":8081",
  "default_server_cert": "/app/integration/testdata/localhost-cert.pem",
  "default_server_key": "/app/integration/testdata/localhost-key.pem",
  "log_console_level": 4,
  "log_file_level": 4,
  "logs_dir": "./logs"
